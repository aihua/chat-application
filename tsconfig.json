{
    "version": "1.6.0",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true
    },
    "filesGlob": [
        "./app/**/*.ts",
        "./typing/**/*.ts"
    ],
    "compileOnSave": false
}
